<?xml version="1.0" encoding="utf-8"?><odoo>    <data>       <!-- Inherit Form View to Modify it -->       <record id="account_account_form_inherit" model="ir.ui.view">           <field name="name">account.account.form.inherit</field>           <field name="model">account.account</field>           <field name="inherit_id" ref="account.view_account_form"/>           <field name="arch" type="xml">               <xpath expr="//group[@name='right_main_group']/field[@name='deprecated']" position="after">				   <field name="limit_balance"/>				   <field name="total_balance"/>               </xpath>           </field>       </record>    </data></odoo>